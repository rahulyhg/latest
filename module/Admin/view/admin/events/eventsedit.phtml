                        },
                        success: function (resp) {
                            if (resp.Status == 'Success') {
                                $("#event_city").empty();
                                $("#event_branch_id").empty();
                                $("#event_city").append("<option value=''>Select City</option>");
                                $.each(resp.statelist, function (idx, obj) {
                                    $("#event_city").append("<option value='" + obj["id"] + "'>" + obj["city_name"] + "</option>");
                                });
                            } else {
                                $("#event_city").empty();
                                alert("No Cities are Available");
                            }
                        },
                        error: function (error) {
                            console.log(error);
                            alert(error);
                        }
                    });
                });
                /*******************End******************/
