                /**************Get State on select Country***************/
                $("#event_country").on('change', function () {
                    var countryId = this.value;
                    //alert(data);
                    $.ajax({
                        url: '/admincontrol/' + 'events/getStateName',
                        //url:'events/getStateName',
                        type: "POST",
                        dataType: "json",
                        data: {Country_ID: countryId},
                        beforeSend: function () {
                            $('#Loading_Request').show();
                        },
                        complete: function () {
                            $('#Loading_Request').hide();
                        },
                        success: function (resp) {
                            if (resp.Status == 'Success') {
                                $("#event_state").empty();
                                $("#event_city").empty();
                                $("#event_state").append("<option value=''>Select State</option>");
                                $.each(resp.statelist, function (idx, obj) {
                                    $("#event_state").append("<option value='" + obj["id"] + "'>" + obj["state_name"] + "</option>");
                                });
                            } else {
                                $("#event_state").empty();
                                $("#event_city").empty();
                                alert("No States are available");
                            }
                        },
                        error: function (error) {
                            console.log(error);
                            alert(error);
                        }
                    });
                });
                /*******************End******************/
                /*************Get City on Select State*************************************************/
                $("#event_state").on('change', function () {
                    var stateId = this.value;
                    $.ajax({
                        url: '/admincontrol/' + 'events/getCityName',
